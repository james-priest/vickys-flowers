{
  "name": "vickys-flowers",
  "version": "1.0.0",
  "browser": "index.html",
  "repository": "https://github.com/james-priest/vickys-flowers.git",
  "author": "James Priest <james-priest@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm dist/*",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-dist": "0.0.6",
    "parcel-plugin-imagemin": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.6.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "normalize.css": "^8.0.1"
  },
  "browserslist": [
    "last 2 version",
    "> 2%",
    "not dead"
  ],
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "grid": "autoplace"
      },
      "postcss-preset-env": true
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  }
}
